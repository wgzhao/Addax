{
  "entry": {
    "jvm": "-Xms32M -Xmx1G",
    "environment": {}
  },
  "common": {
    "column": {
      "datetimeFormat": "yyyy-MM-dd HH:mm:ss",
      "timeFormat": "HH:mm:ss",
      "dateFormat": "yyyy-MM-dd",
      "extraFormats": [
        "yyyyMMdd"
      ],
      "timeZone": "PRC",
      "encoding": "utf-8"
    }
  },
  "core": {
    "addaxServer": {
      "address": ""
    },
    "transport": {
      "channel": {
        "speed": {
          "byte": -1,
          "record": -1
        },
        "flowControlInterval": 20,
        "capacity": 512,
        "byteCapacity": 67108864
      }
    },
    "container": {
      "job": {
        "reportInterval": 10000,
        "sleepInterval": 3000
      },
      "taskGroup": {
        "channel": 5
      }
    }
  }
}
