{
  "name": "kuduwriter",
  "parameter": {
    "masterAddress": "127.0.0.1:7051,127.0.0.1:7151,127.0.0.1:7251",
    "timeout": 60,
    "table": "users",
    "writeMode": "upsert",
    "column": [
      {
        "name": "user_id",
        "type": "int8"
      },
      {
        "name": "user_name",
        "type": "string"
      },
      {
        "name": "salary",
        "type": "double"
      }
    ],
    "batchSize": 1024,
    "bufferSize": 2048,
    "skipFail": false,
    "encoding": "UTF-8"
  }
}