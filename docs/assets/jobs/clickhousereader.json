{
  "job": {
    "setting": {
      "speed": {
        "channel": 1,
        "bytes": -1
      },
      "errorLimit": {
        "record": 0,
        "percentage": 0.02
      }
    },
    "content": {
      "reader": {
        "name": "clickhousereader",
        "parameter": {
          "username": "root",
          "password": "root",
          "column": [
            "*"
          ],
          "connection": [
            {
              "table": [
                "ck_addax"
              ],
              "jdbcUrl": [
                "jdbc:clickhouse://127.0.0.1:8123/default"
              ],
              "where": "EVENT_TIME>='2022-01-01 00:00:00.000' and EVENT_TIME<'2022-01-11 00:00:00.000'"
            }
          ]
        }
      },
      "writer": {
        "name": "streamwriter",
        "parameter": {
          "print": true
        }
      }
    }
  }
}
