{
  "job": {
    "content": {
      "reader": {
        "name": "influxdb2reader",
        "parameter": {
          "column": [
            "location",
            "height",
            "wet"
          ],
          "connection": [
            {
              "endpoint": "http://localhost:8086",
              "bucket": "test",
              "table": [
                "temperature"
              ],
              "org": "com.wgzhao"
            }
          ],
          "token": "YOUR_SECURE_TOKEN",
          "range": [
            "-1h",
            "-5m"
          ],
          "limit": 10
        }
      },
      "writer": {
        "name": "streamwriter",
        "parameter": {
          "print": "true"
        }
      }
    },
    "setting": {
      "speed": {
        "bytes": -1,
        "channel": 1
      }
    }
  }
}
